{
  "name": "@actvalue/mysql-redis-cache",
  "description": "wrapper for MySQL queries with Redis caching",
  "version": "2.1.0",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint src --fix",
    "pretest": "docker-compose -f tests/redis-compose.yml up -d",
    "test": "vitest",
    "posttest": "docker-compose -f tests/redis-compose.yml down --volumes",
    "build": "rm -rf dist && tsup src/index.ts --dts --format esm,cjs"
  },
  "author": "ActValue",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "mysql": "^2.18.1",
    "redis": "^4.5.1"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/coverage-c8": "^0.25.8",
    "eslint": "^8.29.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  }
}
