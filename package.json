{
  "name": "@actvalue/mysql-redis-cache",
  "description": "wrapper for MySQL queries with Redis caching",
  "version": "2.0.4",
  "main": "lib/commonjs/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "require": "./lib/commonjs/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint src --fix",
    "pretest": "docker-compose -f tests/redis-compose.yml up -d",
    "test": "jest",
    "posttest": "docker-compose -f tests/redis-compose.yml down --volumes",
    "build": "rm -rf lib/ && tsc && tsc --build tsconfig.es2020.json"
  },
  "author": "ActValue",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "mysql": "^2.18.1",
    "redis": "^4.5.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
