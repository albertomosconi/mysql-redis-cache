{
  "name": "@actvalue/mysql-redis-cache",
  "description": "wrapper for MySQL queries with Redis caching",
  "version": "4.0.0",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint src --fix",
    "test:unit": "vitest run tests/index.test.ts",
    "test:integration": "vitest run tests/mysql.test.ts",
    "test:interop": "vitest run tests/interop.test.ts",
    "test": "vitest run",
    "build": "rm -rf dist && tsup src/index.ts --dts --format esm,cjs"
  },
  "author": "ActValue",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "mysql2": "^3.15.2",
    "redis": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^22.x",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
